{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "$metadata": {
    "uris": {
      "jsonLdContext": "https://raw.githubusercontent.com/NexeraProtocol/vc-schemas/main/examples/id3/v3/id3.jsonld",
      "jsonSchema": "https://raw.githubusercontent.com/NexeraProtocol/vc-schemas/main/examples/id3/v3/id3.json"
    }
  },
  "required": [
    "@context",
    "id",
    "type",
    "issuanceDate",
    "credentialSubject",
    "credentialSchema",
    "credentialStatus",
    "issuer"
  ],
  "properties": {
    "@context": {
      "type": ["string", "array", "object"]
    },
    "id": {
      "type": "string"
    },
    "type": {
      "type": ["string", "array"],
      "items": {
        "type": "string"
      }
    },
    "issuer": {
      "type": ["string", "object"],
      "format": "uri",
      "required": ["id"],
      "properties": {
        "id": {
          "type": "string",
          "format": "uri"
        }
      }
    },
    "issuanceDate": {
      "type": "string",
      "format": "date-time"
    },
    "expirationDate": {
      "type": "string",
      "format": "date-time"
    },
    "credentialSchema": {
      "type": "object",
      "required": ["id", "type"],
      "properties": {
        "id": {
          "type": "string",
          "format": "uri"
        },
        "type": {
          "type": "string"
        }
      }
    },
    "subjectPosition": {
      "type": "string",
      "enum": ["none", "index", "value"]
    },
    "merklizationRootPosition": {
      "type": "string",
      "enum": ["none", "index", "value"]
    },
    "revNonce": {
      "type": "integer"
    },
    "version": {
      "type": "integer"
    },
    "updatable": {
      "type": "boolean"
    },
    "credentialSubject": {
      "type": "object",
      "properties": {
        "journeyId": {
          "type": "string"
        },
        "amendmentsData": {
          "type": "object",
          "properties": {
            "data": {
              "type": "object"
            },
            "authenticateSPResult": {
              "type": "object",
              "properties": {
                "AuthenticationID": { "type": "string" },
                "Timestamp": { "type": "string", "format": "date-time" },
                "CustomerRef": { "type": "string" },
                "ProfileID": { "type": "string" },
                "ProfileName": { "type": "string" },
                "ProfileVersion": { "type": "integer" },
                "ProfileRevision": { "type": "integer" },
                "ProfileState": { "type": "string" },
                "ResultCodes": {
                  "type": "object",
                  "properties": {
                    "GlobalItemCheckResultCodes": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "Name": { "type": "string" },
                          "Description": { "type": "string" },
                          "Comment": {
                            "type": "object"
                          },
                          "Match": {
                            "type": "object"
                          },
                          "Warning": {
                            "type": "object"
                          },
                          "ID": { "type": "integer" },
                          "Pass": { "type": "string" },
                          "Address": { "type": "string" },
                          "Forename": { "type": "string" },
                          "Surname": { "type": "string" },
                          "DOB": { "type": "string" },
                          "Alert": { "type": "string" },
                          "SanctionsMatches": {
                            "type": "object"
                          }
                        },
                        "required": [
                          "Name",
                          "Description",
                          "Comment",
                          "ID",
                          "Pass",
                          "Address",
                          "Forename",
                          "Surname",
                          "DOB",
                          "Alert"
                        ]
                      }
                    }
                  },
                  "required": ["GlobalItemCheckResultCodes"],
                  "additionalProperties": false
                },
                "Score": { "type": "integer" },
                "BandText": { "type": "string" },
                "Country": { "type": "string" }
              },
              "required": [
                "AuthenticationID",
                "Timestamp",
                "CustomerRef",
                "ProfileID",
                "ProfileName",
                "ProfileVersion",
                "ProfileRevision",
                "ProfileState",
                "ResultCodes",
                "Score",
                "BandText",
                "Country"
              ]
            },
            "pepIntelligenceResult": {
              "type": "array",
              "items": { "type": "object" }
            },
            "sanctionsEnforcementsResult": {
              "type": "array",
              "items": { "type": "object" }
            }
          },
          "required": [
            "data",
            "authenticateSPResult",
            "pepIntelligenceResult",
            "sanctionsEnforcementsResult"
          ],
          "additionalProperties": false
        },
        "originalData": {
          "type": "object",
          "properties": {
            "data": {
              "type": "object"
            },
            "authenticateSPResult": {
              "type": "object"
            },
            "pepIntelligenceResult": {
              "type": "array",
              "items": { "type": "object" }
            },
            "sanctionsEnforcementsResult": {
              "type": "array",
              "items": { "type": "object" }
            }
          },
          "required": [
            "data",
            "authenticateSPResult",
            "pepIntelligenceResult",
            "sanctionsEnforcementsResult"
          ],
          "additionalProperties": false
        },
        "personalDetails": {
          "type": "object",
          "properties": {
            "citizenship": {
              "type": "string"
            },
            "documentType": {
              "type": "string"
            },
            "firstName": {
              "type": "string"
            },
            "lastName": {
              "type": "string"
            },
            "gender": {
              "type": "string"
            },
            "addressLine1": {
              "type": "string",
              "nullable": true
            },
            "addressLine2": {
              "type": "string",
              "nullable": true
            },
            "birthday": {
              "type": "string",
              "format": "date"
            },
            "placeOfBirth": {
              "type": "string"
            },
            "countryOfResidence": {
              "type": "string"
            },
            "age": {
              "type": "string"
            }
          },
          "required": [
            "citizenship",
            "documentType",
            "firstName",
            "lastName",
            "gender",
            "birthday",
            "placeOfBirth",
            "countryOfResidence",
            "age"
          ],
          "additionalProperties": false
        },
        "id": {
          "type": "string"
        }
      },
      "required": [
        "journeyId",
        "amendmentsData",
        "originalData",
        "personalDetails",
        "id"
      ],
      "additionalProperties": false
    }
  }
}
